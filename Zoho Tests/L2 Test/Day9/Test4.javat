import java.util.Scanner;

class Test3{
	public static void main(String[] args){
	
	Scanner scan=new Scanner(System.in);
	
	System.out.println("Enter the Matrix size...");
	int n=scan.nextInt();
	
	int a[][]=new int [n][n];

	for(int i=0;i<n;i++)
	{
		for(int j=0;j<n;j++)
		{
			a[i][j]=scan.nextInt();
		}
	}
	}
	}